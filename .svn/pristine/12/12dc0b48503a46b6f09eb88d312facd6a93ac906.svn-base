.program .programbox {
    background: #fff;
    border-radius: 10px;
    border: 1px solid #E1E1E1 !important;
    padding: 10px;
    -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,0.05);
    -moz-box-shadow: inset 0 1px 1px rgba(0,0,0,0.05);
    box-shadow: inset 0 1px 1px rgba(0,0,0,0.05);
}

.programbox {
    margin-bottom: 15px;
    border: 1px dotted #ddd;
    .border-radius(4px);
    padding: 5px;
}
.programbox:hover{
  color: #333333;
  background-color: #e6e6e6;
  *background-color: #d9d9d9;
}

.programlogo{
    text-align: left;
    /*box-shadow: 5px 5px 5px #888;*/
}
.programcompany .programlistlogo,
.programcourse .courselogo{
    text-align: center;
}

.programlogo img {
    width: 276px;
    height: 150px;
    box-shadow: 5px 5px 5px #888
}

.programcompany .programlistlogo img,
.programlistlogo img {
    max-height: 150px !important;
    width: 267px;
    height: 150px;
}
.programcourse .courselogo img {
    max-height: 160px !important;
    width: 192px;
    height: 146px;
}

.program .programbox.even,
.program .programbox.odd {
    background: #fff;
    margin-bottom: 5px;
}
.program_info {
  background: #f7f7f7;
  border-bottom: 1px solid #bfbfbf;
  border-right: 1px solid #bfbfbf;
  border-left: 1px solid #bfbfbf;
  height: 70px;
  line-height: 24px;
  font-weight: bold;
  padding: 5px;
  color: #2b2b2b;
}
.program .summary{
  float:right;
  line-height: 15px;
  width: 50%;
}
.program .action{
  text-align: right;
  width: 100%;
}
.page-program-list {
    background: url([[pix:theme|bg/dot]]) left top repeat-x;
    background-size: 4px 20px;
    padding-top: 25px;
}

.frontpage-program-logo{
  /*background: rgba(243, 243, 243, 0.55);*/
  /*background: rgb(243, 243, 243);*/
  /*background:#f4f4f4;*/
  padding: 3px 10px;
  /*border: 1px solid #e7e7e7;*/
  margin-bottom: 5px;
  border-radius: 5px;
  border-bottom: 3px solid #eee;
}
/*E4-141*/
@media (min-width: 1200px) {
    .frontpage-program-logo .row-fluid .span3-frontpage{
        width:23.984%;
    }

    .frontpage-program-logo .row-fluid [class*="span3-frontpage"]{
        margin-left:1.354102564102564%;
    }
}
@media (min-width: 980px) and (max-width:1200px) {
    .frontpage-program-logo .row-fluid .span3-frontpage{
        width:23.984%;
    }

    .frontpage-program-logo .row-fluid [class*="span3-frontpage"]{
        margin-left:1.354102564102564%;
    }
}
@media (min-width: 767px) and (max-width:980px) {
    .frontpage-program-logo .row-fluid .span3-frontpage{
        width:23.981%;
    }
    .frontpage-program-logo .row-fluid [class*="span3-frontpage"]{
        margin-left:1.354102564102564%;
    }
}


.programcompany .title{
  padding-left: 5px;
  padding-right: 5px;
  font-size: 1.5em; /*24px;*/
  color:#000000;
  /*height: 41px;*/
}

.programcompany{
    /*width: 266px;*/
    background:#fff;
    /*padding:3px 10px;*/
    border:1px solid #e7e7e7;
    margin-bottom:10px;
    border-radius:5px;
    border-bottom:3px solid #eee;
    box-shadow: 5px 5px 5px #888888;
}
.programcourse{
    /*width: 230px;*/
    background:#fff;
    /*padding:3px 10px;*/
    border:1px solid #e7e7e7;
    margin-bottom:10px;
    border-radius:5px;
    border-bottom:3px solid #eee;
    box-shadow: 5px 5px 5px #888888;
}
.ellipsis{
    height: 89px;
    padding-left: 5px;
    padding-right: 5px;
    white-space: nowrap;
    overflow : hidden; 
}

.ellipsis .multiline {
    font-size: 1em;/*16px;*/
    font-family: fantasy;
    line-height: 20px;
    white-space: normal;
    color: #929292;
    height: 89px;   
}


.programcourse .coursename{
    font-size: 1.125em;/*18px;*/
    font-weight: bold;
    color: #707070;
    text-align: center;
    height: 25px;
    overflow : hidden;
}
.programcourse .coursedate{
    font-size: 0.75em;/*12px;*/
    font-weight: bold;
    color: #707070;
    text-align: center;
    overflow : hidden;
    height: 25px;
}
.separator{
    width:100%;
    padding-bottom: 20px;
    /*margin-bottom: 60px;*/
    margin-top: 20px;
    /*line-height: 50px;*/
}
.separator-right{
    float :right;
    width:30%;
    color: #777;
    background: url([[pix:theme|bg/dot]]) left center repeat-x;
}
.separator-left{
    float :left;
    width:30%;
    color: #777;
    background: url([[pix:theme|bg/dot]]) right center repeat-x;
}
.separator-text{
    color:#373737;
    width:40%;
    text-align: center;
    float:right;
    font-size:1.5em; /*24px;*/
    font-weight: bold;
}